   "source": [
    "#Grafic evolució dades Gini, lloguer i renda\n",
    "categories_dict = {\n",
    "    6: 'Molt alta Densitat',\n",
    "    7: 'Molt alta Densitat',\n",
    "    10: 'Molt alta Densitat',\n",
    "    11: 'Molt alta Densitat',\n",
    "    5: 'Alta densitat',\n",
    "    8: 'Alta densitat',\n",
    "    9: 'Alta densitat',\n",
    "    15: 'Alta densitat',\n",
    "    31: 'Alta densitat',\n",
    "    68: 'Alta densitat',\n",
    "    1: 'Densitat mitjana',\n",
    "    2: 'Densitat mitjana',\n",
    "    3: 'Densitat mitjana',\n",
    "    4: 'Densitat mitjana',\n",
    "    14: 'Densitat mitjana',\n",
