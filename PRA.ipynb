    "    32: 'Densitat mitjana',\n",
    "    33: 'Densitat mitjana',\n",
    "    64: 'Densitat mitjana',\n",
    "    66: 'Densitat mitjana',\n",
    "    67: 'Densitat mitjana',\n",
    "    69: 'Densitat mitjana',\n",
    "    }\n",
    "#Definim a partir del dictat de categories els codis per densitat baixa\n",
    "for i in range(1, 73):  \n",
    "    if i not in categories_dict:\n",
    "        categories_dict[i] = 'Densitat Baixa'\n",
    "\n",
    "#Funció per categoritzar codis de barris\n",
    "def classify_densitat(code):\n",
    "    return categories_dict.get(code, 'Densitat Baixa')\n",
    "\n",
    "# Carpetes arxius de les tres variables\n",
    "folders = {\n",
    "    'gini': 'data/gini',\n",
    "    'lloguer': 'data/lloguer',\n",
    "    'renda': 'data/renda'\n",
    "}\n",
    "\n",
    "#Dataframes per variable\n",
    "df_final_gini = pd.DataFrame()\n",
    "df_final_lloguer = pd.DataFrame()\n",
    "df_final_renda = pd.DataFrame()\n",
    "\n",
    "for category, path in folders.items():\n",
    "    df_list = []\n",
    "    #Iteració sobre arxius de carpeta\n",
    "    for file_name in os.listdir(path):\n",
    "        if file_name.endswith('.csv'): \n",
    "            file_path = os.path.join(path, file_name)\n",
    "            df_temp = pd.read_csv(file_path, delimiter=',') \n",
    "            # Classificació codis de barris\n",
    "            df_temp['Categoria_Densitat'] = df_temp['Codi_Barri'].apply(classify_densitat)\n",
    "            # Agragació tipus variables\n",
    "            df_temp['Variable'] = category.capitalize()\n",
    "            df_list.append(df_temp)\n",
    "\n",
    "    #Concatenació dataframes per variable\n",
    "    df_category = pd.concat(df_list, ignore_index=True)\n",
    "\n",
    "    #Generació dataframe per variable\n",
    "    if category == 'gini':\n",
    "        df_final_gini = df_category\n",
    "    elif category == 'lloguer':\n",
    "        df_final_lloguer = df_category\n",
    "    elif category == 'renda':\n",
    "        df_final_renda = df_category\n",
    "\n",
    "# Asegúrate de establecer la opción de visualización\n",
    "#pd.set_option('display.max_rows', None)\n",
    "#pd.set_option('display.max_columns', None)"
   ]
